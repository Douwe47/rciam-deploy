# {{ ansible_managed }}

[Unit]
Description=SSP deployer service {{ build_branch }}

[Service]
User=root
ExecStart={{ fedreg_agent_venv_path }}/bin/deployer_ssp -c {{ fedreg_agent_conf_dir }}/{{ deployers_config_name }}
Restart=always
RestartSec=60s
Type=simple

[Install]
WantedBy=multi-user.target
